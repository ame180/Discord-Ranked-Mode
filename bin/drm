#!/usr/bin/env php
<?php

use Symfony\Component\Console\Application;

require_once __DIR__ . '/../config/bootstrap.php';
$container = require __ROOT__ . '/config/container.php';

$application = new Application('Discord Ranked Mode');

$application->add($container->get(\App\Command\GetPlayerRanks::class));
$application->add($container->get(\App\Command\UpdatePlayers::class));

$application->run();